{
  "name": "samuelpietro/cnpjutils",
  "description": "Funções úteis para gerar e validar números de CNPJ no novo padrão alfanumérico.",
  "type": "library",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "CNPJUtils\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Samuel Pietro",
      "email": "samuelpietro.b@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-text --coverage-html=coverage-html --coverage-clover=coverage.xml",
    "cs-fix": "vendor/bin/php-cs-fixer fix",
    "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
    "analyse": "vendor/bin/phpstan analyse",
    "ci": [
      "@cs-check",
      "@analyse",
      "@test"
    ]
  },
  "keywords": [
    "cnpj",
    "validar",
    "alfanumerico",
    "gerar",
    "brasil"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  },
  "homepage": "https://github.com/SamuelPietro/cnpjutils",
  "support": {
    "issues": "https://github.com/SamuelPietro/cnpjutils/issues",
    "source": "https://github.com/SamuelPietro/cnpjutils",
    "packagist": "https://packagist.org/packages/samuelpietro/cnpjutils"
  }
}